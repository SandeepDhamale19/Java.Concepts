<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Retry Failed Test Automation Suite" verbose="1">

	<listeners>
		<listener
			class-name="com.testautomation.testng.AnnotationTransformer" />
	</listeners>

	<test name="RetryMultipleFailedTests">
		<classes>
			<class name="com.testautomation.testng.TestNGRetry" />
		</classes>
	</test>
</suite> 
